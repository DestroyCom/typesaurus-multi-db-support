{
  "name": "typesaurus",
  "version": "10.0.0-beta.12",
  "description": "Type-safe ODM for Firestore",
  "keywords": [
    "Firebase",
    "Firestore",
    "TypeScript"
  ],
  "type": "module",
  "browser": "./adapter/web",
  "main": "./adapter/admin/index.js",
  "module": "./adapter/admin/index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "browser": "./adapter/web/index.mjs",
      "require": "./adapter/admin/index.js",
      "import": "./adapter/admin/index.mjs",
      "types": "./index.d.ts"
    },
    "./batch": {
      "browser": "./adapter/web/batch.mjs",
      "require": "./adapter/admin/batch.js",
      "import": "./adapter/admin/batch.mjs",
      "types": "./batch/index.d.ts"
    },
    "./groups": {
      "browser": "./adapter/web/groups.mjs",
      "require": "./adapter/admin/groups.js",
      "import": "./adapter/admin/groups.mjs",
      "types": "./groups/index.d.ts"
    },
    "./transaction": {
      "browser": "./adapter/web/transaction.mjs",
      "require": "./adapter/admin/transaction.js",
      "import": "./adapter/admin/transaction.mjs",
      "types": "./transaction/index.d.ts"
    },
    "./helpers": {
      "browser": "./helpers/index.mjs",
      "require": "./helpers/index.js",
      "import": "./helpers/index.mjs",
      "types": "./helpers/index.d.ts"
    },
    "./adapter/next/admin": {
      "default": "./adapter/admin/index.mjs",
      "types": "./index.d.ts"
    },
    "./adapter/next/web": {
      "default": "./adapter/web/index.mjs",
      "types": "./index.d.ts"
    }
  },
  "sideEffects": false,
  "repository": "https://github.com/kossnocorp/typesaurus",
  "author": "Sasha Koss <koss@nocorp.me>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.19.0",
    "@types/sinon": "^10.0.13",
    "@types/webpack-env": "^1.18.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-add-import-extension": "^1.6.0",
    "firebase": "^10.7.0",
    "firebase-admin": "^11.11.1",
    "firebase-tools": "^12.9.1",
    "jest": "^29.7.0",
    "karma": "^6.0.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "mocha": "^6.2.0",
    "prettier": "^3.1.1",
    "puppeteer": "^16.2.0",
    "sinon": "^14.0.0",
    "size-limit": "^8.0.0",
    "size-limit-preset-node-lib": "^0.1.0",
    "typedoc": "^0.15.0",
    "typescript": "^5.3.3",
    "webpack": "^5.74.0"
  }
}
